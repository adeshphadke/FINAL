def intro():
  print("My name is Adesh Phadke")
  print("I am from pune, Maharashtra")

def edit(intro):
  def inner():
    print("Hi")
    return intro
  return inner
intro = edit(intro)
intro()
def greet():
  print("Good morning")
greet()
class Student:
  def __init__(self):
    print("Ok")
s1 = Student()
